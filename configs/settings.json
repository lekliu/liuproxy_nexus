{
  "gateway": {
    "sticky_session_mode": "disabled",
    "sticky_session_ttl": 300,
    "sticky_rules": [
      "*.google.com",
      "baidu.com"
    ],
    "load_balancer_strategy": "round_robin"
  },
  "routing": {
    "rules": [
      {
        "priority": 99,
        "type": "domain",
        "value": [
          "baifi.com"
        ],
        "target": "DIRECT"
      }
    ]
  },
  "logging": {},
  "firewall": {
    "enabled": true,
    "rules": [
      {
        "priority": 89,
        "source_cidr": [
          "183.89.2.3"
        ],
        "action": "deny"
      },
      {
        "priority": 100,
        "source_cidr": [
          "127.0.1.28"
        ],
        "action": "allow"
      },
      {
        "priority": 9999,
        "action": "allow"
      }
    ]
  }
}